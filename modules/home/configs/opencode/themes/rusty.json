{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "foreground": "#c5c8c6",
    "background": "#0f0f0f",
    "panel": "#1d1f21",
    "selection": "#373b41",
    "line": "#282a2e",
    "diff_background": "#494e56",
    "comment": "#969896",
    "red": "#cc6666",
    "orange": "#de935f",
    "yellow": "#f0c674",
    "green": "#b5bd68",
    "aqua": "#8abeb7",
    "blue": "#81a2be",
    "purple": "#b294bb",
    "window": "#4d5057"
  },
  "theme": {
    "primary": {
      "dark": "orange",
      "light": "orange"
    },
    "secondary": {
      "dark": "blue",
      "light": "blue"
    },
    "accent": {
      "dark": "aqua",
      "light": "aqua"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "orange",
      "light": "orange"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "aqua",
      "light": "aqua"
    },
    "text": {
      "dark": "foreground",
      "light": "background"
    },
    "textMuted": {
      "dark": "comment",
      "light": "comment"
    },
    "background": {
      "dark": "background",
      "light": "foreground"
    },
    "backgroundPanel": {
      "dark": "panel",
      "light": "selection"
    },
    "backgroundElement": {
      "dark": "selection",
      "light": "line"
    },
    "border": {
      "dark": "window",
      "light": "window"
    },
    "borderActive": {
      "dark": "orange",
      "light": "orange"
    },
    "borderSubtle": {
      "dark": "selection",
      "light": "selection"
    },
    "diffAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "comment",
      "light": "comment"
    },
    "diffHunkHeader": {
      "dark": "window",
      "light": "window"
    },
    "diffHighlightAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffHighlightRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffAddedBg": {
      "dark": "diff_background",
      "light": "selection"
    },
    "diffRemovedBg": {
      "dark": "diff_background",
      "light": "selection"
    },
    "diffContextBg": {
      "dark": "line",
      "light": "foreground"
    },
    "diffLineNumber": {
      "dark": "comment",
      "light": "comment"
    },
    "diffAddedLineNumberBg": {
      "dark": "diff_background",
      "light": "selection"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diff_background",
      "light": "selection"
    },
    "markdownText": {
      "dark": "foreground",
      "light": "background"
    },
    "markdownHeading": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownLink": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLinkText": {
      "dark": "aqua",
      "light": "aqua"
    },
    "markdownCode": {
      "dark": "green",
      "light": "green"
    },
    "markdownBlockQuote": {
      "dark": "comment",
      "light": "comment"
    },
    "markdownEmph": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownStrong": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownHorizontalRule": {
      "dark": "window",
      "light": "window"
    },
    "markdownListItem": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownListEnumeration": {
      "dark": "purple",
      "light": "purple"
    },
    "markdownImage": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownImageText": {
      "dark": "aqua",
      "light": "aqua"
    },
    "markdownCodeBlock": {
      "dark": "foreground",
      "light": "background"
    },
    "syntaxComment": {
      "dark": "comment",
      "light": "comment"
    },
    "syntaxKeyword": {
      "dark": "purple",
      "light": "purple"
    },
    "syntaxFunction": {
      "dark": "aqua",
      "light": "aqua"
    },
    "syntaxVariable": {
      "dark": "foreground",
      "light": "background"
    },
    "syntaxString": {
      "dark": "green",
      "light": "green"
    },
    "syntaxNumber": {
      "dark": "orange",
      "light": "orange"
    },
    "syntaxType": {
      "dark": "orange",
      "light": "orange"
    },
    "syntaxOperator": {
      "dark": "foreground",
      "light": "background"
    },
    "syntaxPunctuation": {
      "dark": "foreground",
      "light": "background"
    }
  }
}
